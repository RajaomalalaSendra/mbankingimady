<nav class="navbar navbar-default" style="margin-top: -50px">
      <ul class="nav navbar-nav navbar-left">
        <li><a class="menu active" href="#home" >SignUp To The MbankingImady</a></li>
      </ul>
</nav>
<div class="container">
<div class="bg-default" style="margin-left: 400px; margin-top: 100px">
  <div class="row">
    <div class="col-4">
      
    </div>
    <div class="col-sm-4">
      <h2>Log in</h2>

      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="field">
          <%= f.label :email %><br />
          <%= f.email_field :email, autofocus: true, autocomplete: "email", :class => "form-control rounded" %>
        </div>

        <div class="field">
          <%= f.label :password %><br />
          <%= f.password_field :password, autocomplete: "current-password", :class => "form-control rounded" %>
        </div>

        <% if devise_mapping.rememberable? %>
          <div class="field">
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me %>
          </div>
        <% end %>

        <div class="actions">
          <%= f.submit "Log in", :class => "btn btn-success" %>
        </div>
      <% end %>

      <%= render "devise/shared/links" %>
  </div>
  <div class="col-4">
      
  </div>
  </div>
</div>
</div>